schema {
    query: QueryType
}

type QueryType {
    orders: [Order]
}

type Account {
    id: ID!
    name: String!
    accountNumber: Int!
    createdAt: String
}

type Instrument {
    id: ID!
    name: String!
    market: String!
}

type Order {
    id: ID!
    account: Account!
    instrument: Instrument!
    volume: Float!
    limit: Float!
    createdAt: String
}

